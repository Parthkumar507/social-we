    <h1>
        This is Codial <%=titleName%>
        
    </h1>


    <form action="/post/create" id="new-post-form" method="post">

        <textarea name="content" id="new-post-content" cols="30" rows="5" placeholder="type here..">
    
        </textarea>
        <input type="submit" value="post">
    
    </form>

            <div id="Post-display-div">
                <%for(post of posts){%>
                    <li>
                        <p>
                            <%=post.content%>
                            <br>
                            <%=post.user.name%>
                        </p>

                    </li>

                 <%}%>

            </div>
